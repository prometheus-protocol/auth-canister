{
  "name": "@prometheus-protocol/declarations",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/generated/*",
      "types": "./dist/generated/*"
    }
  },
  "scripts": {
    "build": "npm-run-all clean compile copy:generated",
    "clean": "rimraf dist",
    "compile": "tsc",
    "copy:generated": "cpx \"src/generated/**/*\" \"dist/generated\""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1"
  }
}
